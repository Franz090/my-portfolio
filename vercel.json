{
    "version": 2,
    "builds": [
       {
         "src": "index.html",
         "use": "@vercel/static",
         "config": {
           "distDir": "dist",
           "trailingSlash": true
         }
       }
    ],
    "routes": [
       {
         "src": "/(.*)",
         "dest": "/index.html"
       }
    ]
   }